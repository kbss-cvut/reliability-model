###  http://onto.fel.cvut.cz/ontologies/nasa-fta/analyzes
:analyzes rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAnalizedBy ;
          rdfs:domain :Explanation ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :ElementBehavior
                                     :Function
                                     :PerformanceConstraint
                                   )
                     ] ;
          rdfs:label "analyzes" .


###  http://onto.fel.cvut.cz/ontologies/nasa-fta/causes
:causes rdf:type owl:ObjectProperty ;
        owl:inverseOf :effect_of ;
        rdfs:domain :ElementBehavior ;
        rdfs:range :ElementBehavior ;
        rdfs:comment "derived"@en ;
        rdfs:label "causes" .


###  http://onto.fel.cvut.cz/ontologies/nasa-fta/characterizes
:characterizes rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Likelihood
                                           :MissinoImpact
                                           :PerformanceConstraint
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :Performance
                                          :ViolationExplanations
                                        )
                          ] .


###  http://onto.fel.cvut.cz/ontologies/nasa-fta/constrains
:constrains rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :ElementBehavior
                                        :PerformanceConstraint
                                      )
                        ] ;
            rdfs:range :StateVariable .


###  http://onto.fel.cvut.cz/ontologies/nasa-fta/detects
:detects rdf:type owl:ObjectProperty ;
