@base <http://onto.fel.cvut.cz/ontologies/msmt-fta> .

<http://onto.fel.cvut.cz/ontologies/msmt-fta> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/msmt-fta/has_failur_mode
:has_failur_mode rdf:type owl:ObjectProperty ;
                 rdfs:label "has failur mode" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/has_gate
:has_gate rdf:type owl:ObjectProperty ;
          rdfs:label "has gate" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/has_input
:has_input rdf:type owl:ObjectProperty ;
           rdfs:label "has input" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/has_output
:has_output rdf:type owl:ObjectProperty ;
            rdfs:label "has output" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/has_probability
:has_probability rdf:type owl:ObjectProperty ;
                 rdfs:label "has probability" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/occurenne_of
:occurenne_of rdf:type owl:ObjectProperty ;
              rdfs:label "occurenne of" .


#################################################################
#    Data properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/msmt-fta/probability_value
:probability_value rdf:type owl:DatatypeProperty ;
                   rdfs:range owl:real .


#################################################################
#    Classes
#################################################################

###  http://onto.fel.cvut.cz/ontologies/msmt-fta/and_gate
:and_gate rdf:type owl:Class ;
          rdfs:subClassOf :gate ;
          rdfs:label "and gate" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/component
:component rdf:type owl:Class ;
           rdfs:subClassOf :system ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :has_failur_mode ;
                             owl:someValuesFrom :failure_mode
                           ] .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/failure_mode
:failure_mode rdf:type owl:Class ;
              rdfs:label "failure mode" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/fta_event
:fta_event rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :has_probability ;
                             owl:someValuesFrom :likelihood
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :occurenne_of ;
                             owl:someValuesFrom :failure_mode
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :has_gate ;
                             owl:allValuesFrom :gate
                           ] ;
           rdfs:label "fta event" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/gate
:gate rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Class ;
                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                        owl:onProperty :has_input ;
                                        owl:someValuesFrom :intermidiate-derived_event
                                      ]
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :has_input ;
                                        owl:someValuesFrom :primary-risk_event
                                      ]
                                    )
                      ] ,
                      [ rdf:type owl:Class ;
                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                        owl:onProperty :has_output ;
                                        owl:someValuesFrom :intermidiate-derived_event
                                      ]
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :has_output ;
                                        owl:someValuesFrom :undeveloped-initial_event
                                      ]
                                    )
                      ] ;
      rdfs:label "gate" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/intermidiate-derived_event
:intermidiate-derived_event rdf:type owl:Class ;
                            rdfs:subClassOf :fta_event ;
                            rdfs:label "intermidiate - derived event" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/likelihood
:likelihood rdf:type owl:Class ;
            rdfs:label "likelihood" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/or_gate
:or_gate rdf:type owl:Class ;
         rdfs:subClassOf :gate ;
         rdfs:label "or gate" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/primary-risk_event
:primary-risk_event rdf:type owl:Class ;
                    rdfs:subClassOf :fta_event ;
                    rdfs:label "primary - risk event" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/system
:system rdf:type owl:Class ;
        rdfs:label "system" .


###  http://onto.fel.cvut.cz/ontologies/msmt-fta/undeveloped-initial_event
:undeveloped-initial_event rdf:type owl:Class ;
                           rdfs:subClassOf :fta_event ;
                           rdfs:label "undeveloped - initial event" .


