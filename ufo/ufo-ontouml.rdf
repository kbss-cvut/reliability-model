    <!-- http://onto.fel.cvut.cz/ontologies/ufo/kind -->

    <owl:Class rdf:about="http://onto.fel.cvut.cz/ontologies/ufo/kind">
        <rdfs:subClassOf rdf:resource="http://onto.fel.cvut.cz/ontologies/ufo/substance-sortal"/>
        <terms:description xml:lang="en">A «kind» represents a substance sortal whose instances are functional complexes. Examples include instances of Natural Kinds (such as Person, Dog, Tree) and of artifacts (Chair, Car, Television).</terms:description>
        <rdfs:label xml:lang="en">Kind</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://onto.fel.cvut.cz/ontologies/ufo/kind"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
