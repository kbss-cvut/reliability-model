        <terms:source>ofscm</terms:source>
    </owl:Axiom>
    


    <!-- http://onto.fel.cvut.cz/ontologies/ufo/mixin -->

    <owl:Class rdf:about="http://onto.fel.cvut.cz/ontologies/ufo/mixin">
        <rdfs:subClassOf rdf:resource="http://onto.fel.cvut.cz/ontologies/ufo/non-rigid-mixin"/>
        <terms:description xml:lang="en">A «mixin» represents properties which are essential to some of its instances and accidental to others (semi-rigidity). An example is the mixin Seatable, which represents a property that can be considered essential to the kinds Chair and Stool, but accidental to Crate, Paper Box or Rock.</terms:description>
        <rdfs:label xml:lang="en">Mixin</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://onto.fel.cvut.cz/ontologies/ufo/mixin"/>
        <owl:annotatedProperty rdf:resource="http://purl.org/dc/terms/description"/>
        <owl:annotatedTarget xml:lang="en">A «mixin» represents properties which are essential to some of its instances and accidental to others (semi-rigidity). An example is the mixin Seatable, which represents a property that can be considered essential to the kinds Chair and Stool, but accidental to Crate, Paper Box or Rock.</owl:annotatedTarget>
        <terms:source>ofscm</terms:source>
    </owl:Axiom>
    <owl:Axiom>
