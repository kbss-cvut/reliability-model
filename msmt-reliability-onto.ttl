@prefix : <http://onto.fel.cvut.cz/ontologies/reliability/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://onto.fel.cvut.cz/ontologies/reliability> .

<http://onto.fel.cvut.cz/ontologies/reliability> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/reliability/causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:label "causes" .


###  http://onto.fel.cvut.cz/ontologies/reliability/has_failure_mode
:has_failure_mode rdf:type owl:ObjectProperty ;
                  rdfs:label "has failure mode" .


###  http://onto.fel.cvut.cz/ontologies/reliability/has_function
:has_function rdf:type owl:ObjectProperty ;
              rdfs:label "has function" .


#################################################################
#    Classes
#################################################################

###  http://onto.fel.cvut.cz/ontologies/reliability/component
:component rdf:type owl:Class ;
           rdfs:subClassOf :subsystem ;
           rdfs:comment "component" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure-mode
:failure-mode rdf:type owl:Class ;
              rdfs:label "failure mode" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mode_cause
:failure_mode_cause rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :causes ;
                                      owl:someValuesFrom :failure-mode
                                    ] ;
                    rdfs:comment "failure mode cause" .


###  http://onto.fel.cvut.cz/ontologies/reliability/fmea
:fmea rdf:type owl:Class ;
      rdfs:subClassOf :relibility_method ;
      rdfs:label "fmea" .


###  http://onto.fel.cvut.cz/ontologies/reliability/fta
:fta rdf:type owl:Class ;
     rdfs:subClassOf :relibility_method ;
     rdfs:label "fta" .


###  http://onto.fel.cvut.cz/ontologies/reliability/function
:function rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :has_failure_mode ;
                            owl:someValuesFrom :failure-mode
                          ] ;
          rdfs:comment "function" .


###  http://onto.fel.cvut.cz/ontologies/reliability/relibility_method
:relibility_method rdf:type owl:Class ;
                   rdfs:label "relibility method" .


###  http://onto.fel.cvut.cz/ontologies/reliability/subsystem
:subsystem rdf:type owl:Class ;
           rdfs:subClassOf :system ;
           rdfs:comment "subsystem" .


###  http://onto.fel.cvut.cz/ontologies/reliability/system
:system rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :has_function ;
                          owl:someValuesFrom :function
                        ] ;
        rdfs:seeAlso "system" .


