###  http://onto.fel.cvut.cz/ontologies/quality/Quality_Criteria
<http://onto.fel.cvut.cz/ontologies/quality/Quality_Criteria> rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo-a-ext/quality> .


###  http://onto.fel.cvut.cz/ontologies/quality/Qulalty_Model
<http://onto.fel.cvut.cz/ontologies/quality/Qulalty_Model> rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo-c-ext/proposition> .


###  http://onto.fel.cvut.cz/ontologies/reliability/component
:component rdf:type owl:Class ;
           rdfs:subClassOf :subsystem ;
           rdfs:comment "component" .


###  http://onto.fel.cvut.cz/ontologies/reliability/effect_mode
:effect_mode rdf:type owl:Class ;
             rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/disposition> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/is-manifested-by> ;
                               owl:someValuesFrom :failure_effect_event
                             ] ;
             rdfs:label "effect mode" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure-mode
:failure-mode rdf:type owl:Class ;
              rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/disposition> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :contributes ;
                                owl:someValuesFrom :effect_mode
                              ] ;
              rdfs:label "failure mode" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_effect_event
:failure_effect_event rdf:type owl:Class ;
                      rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/event> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/brings-about> ;
                                        owl:someValuesFrom <http://onto.fel.cvut.cz/ontologies/fmea/failure-mode-effect>
                                      ] ;
                      rdfs:label "failure effect event" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_event
:failure_event rdf:type owl:Class ;
               rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/event> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :derived_from ;
                                 owl:someValuesFrom :failure-mode
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/brings-about> ;
                                 owl:someValuesFrom :failure_state
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/is-manifested-by> ;
                                 owl:someValuesFrom :failure-mode
                               ] ;
               rdfs:label "failure event" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mechanism
:failure_mechanism rdf:type owl:Class ;
                   rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/event> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/brings-about> ;
                                     owl:someValuesFrom :failure_mode_cause
                                   ] ;
                   rdfs:label "failure mechanism" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mechanism_cause_situation
:failure_mechanism_cause_situation rdf:type owl:Class ;
                                   rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/situation> ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/triggers> ;
                                                     owl:someValuesFrom :failure_mechanism
                                                   ] ;
                                   rdfs:label "failure mechanism cause situation" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mode_cause
:failure_mode_cause rdf:type owl:Class ;
                    rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/situation> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :causes ;
                                      owl:someValuesFrom :failure-mode
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/activates> ;
                                      owl:someValuesFrom :failure-mode
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/triggers> ;
                                      owl:someValuesFrom :failure_event
                                    ] ;
                    owl:disjointWith :safe_situation ;
                    rdfs:comment "failure mode cause" ;
                    rdfs:label "failure mode cause" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mode_mitigation
:failure_mode_mitigation rdf:type owl:Class ;
                         rdfs:subClassOf :function ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :impairs ;
                                           owl:someValuesFrom :failure-mode
                                         ] ;
                         rdfs:label "failure mode mitigation" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_mode_mitigation_event
:failure_mode_mitigation_event rdf:type owl:Class ;
                               rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/fmea/taking-action> ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/brings-about> ;
                                                 owl:someValuesFrom :safe_situation
                                               ] ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/is-manifested-by> ;
                                                 owl:someValuesFrom :failure_mode_mitigation
                                               ] ;
                               rdfs:label "failure mode mitigation event" .


###  http://onto.fel.cvut.cz/ontologies/reliability/failure_state
:failure_state rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/activates> ;
                                     owl:someValuesFrom :effect_mode
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/triggers> ;
                                     owl:someValuesFrom :failure_effect_event
                                   ] ;
               rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/situation> ;
               rdfs:label "failure state" .


###  http://onto.fel.cvut.cz/ontologies/reliability/fmea
:fmea rdf:type owl:Class ;
